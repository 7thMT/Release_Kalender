<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.release_kalender.ui.home.HomeFragment"
        android:label="@string/label_home"
        tools:layout="@layout/fragment_home" />


    <fragment
        android:id="@+id/navigation_trending"
        android:name="com.example.release_kalender.ui.trending.TrendingFragment"
        android:label="@string/label_trending"
        tools:layout="@layout/fragment_trending" />

    <fragment
        android:id="@+id/navigation_calender"
        android:name="com.example.release_kalender.ui.calender.CalenderFragment"
        android:label="@string/label_calender"
        tools:layout="@layout/fragment_calender" />
</navigation>